{
    "name": "SummaryNote",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Summarize Articles and Save to Evernote!",
    "content_scripts": [{
        "matches":["http://*/*"],
        "js": ["js/jquery-2.1.1.js"]
    }]

    "default_locale": "en"
    "icons": {icon.png}
    "browser_action": {
    	"default_title": "SummaryNote"
    	"default_popup": "popup.html"
    	"default_icon": {
    		assets/icon.png
    	}
    }


}




